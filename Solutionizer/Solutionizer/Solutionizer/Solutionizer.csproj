    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="8.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Solutionizer.Agents\Solutionizer.Agents.csproj" />
  </ItemGroup>

</Project>
